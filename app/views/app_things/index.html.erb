<p id="notice"><%= notice %></p>

<h1>Listing App Things</h1>

<table>
  <thead>
    <tr>
      <th>App</th>
      <th>Thing</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @app_things.each do |app_thing| %>
      <tr>
        <td><%= app_thing.app %></td>
        <td><%= app_thing.thing %></td>
        <td><%= link_to 'Show', app_thing %></td>
        <td><%= link_to 'Edit', edit_app_thing_path(app_thing) %></td>
        <td><%= link_to 'Destroy', app_thing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New App thing', new_app_thing_path %>
