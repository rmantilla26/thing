<tr class="fields">
    <%= f.hidden_field :id, label:false %>
    <%= f.hidden_field :_destroy %>
    <td><%= f.input :name_es, label:false %></td>
    <td><%= f.input :name_en, label:false %></td>
    <td><%= f.association :type, label:false, :collection => Type.all, prompt: "Tipo de dato", :label_method => :name, :value_method => :id %></td>
    <td><%= link_to_remove_fields "remove", f %></td>
 </tr>